<body>

    <!-- Start Content -->
    @*<div class="first-container container fluid">*@

        <div id="wrapper">

            <div id="content">
                @*<ul class="breadcrumb">
                    <li><a href="index.html?lang=en" class="glyphicons home"><i></i> AdminPlus</a></li>
                    <li class="divider"></li>
                    <li>My Account</li>
                    <li class="divider"></li>
                    <li>Advanced profile</li>
                </ul>*@
                <div class="separator bottom"></div>

                <h3 class="glyphicons user"><i></i>Advanced profile</h3>
                <div class="innerLR">

                    <!-- Widget -->
                    <div class="widget widget-2 widget-tabs widget-tabs-2 border-bottom-none">

                        <!-- Widget heading -->
                        <div class="widget-head border-bottom">
                            <ul>
                                <li class="active"><a class="glyphicons display" href="#overview" data-toggle="tab"><i></i>Overview</a></li>
                                <li><a class="glyphicons edit" href="#edit-account" data-toggle="tab"><i></i>Edit account</a></li>
                                <li><a class="glyphicons luggage" href="#projects" data-toggle="tab"><i></i>Projects</a></li>
                            </ul>
                        </div>
                        <!-- // Widget heading END -->

                        <div class="widget-body innerT innerLR-none">

                            <form class="form-horizontal margin-none">
                                <div class="tab-content padding-none">
                                    <!-- OVERVIEW TAB SECTION -->
                                    <div class="tab-pane active widget-body-regular" id="overview">
                                        <div class="row">
                                            <!-- PROFILE LEFT SECTION -->
                                            <div class="col-md-3 center">
                                                <!-- Profile Photo -->
                                                <a href=""><img data-src="holder.js/200x150/dark" alt="Profile" class="img-thumbnail" /></a>
                                                <h4>@ViewBag.FullName</h4>
                                                <div class="separator bottom"></div>
                                                <!-- // Profile Photo END -->
                                                <!-- Social Icons -->
                                                @*<a href="" class="social standard primary facebook"><i></i></a>
                                                <a href="" class="social standard twitter"><i></i></a>
                                                <a href="" class="social standard linked_in"><i></i></a>*@
                                                <div class="clearfix separator bottom"></div>
                                                <!-- // Social Icons END -->
                                                <!-- Role Assignments-->
                                                <div>
                                                    <h4>Assigned Roles</h4>
                                                    <table id="userRoles">
                                                        <thead>
                                                            <tr><td>Role</td></tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var role in Model.UserInfo.Roles) {
                                                                <tr>
                                                                    <td>role</td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                            <!-- PROFILE RIGHT SECTION -->
                                            <div class="col-md-9">
                                                <!-- Assigned Tickets Widget -->
                                                <div class="widget widget-4 margin-bottom-none">
                                                    <!-- Widget Heading -->
                                                    <div class="widget-head">
                                                        <h4 class="heading">Assigned Tickets</h4>
                                                    </div>
                                                    <h4>Assigned Tickets</h4>
                                                    <table id="userTickets">
                                                        <thead>
                                                            <tr>
                                                                <th>@Html.DisplayNameFor(model => model.UserInfo.Tickets.First(p => p.Id > 0).Title)</th>
                                                                <th>@Html.DisplayNameFor(model => model.UserInfo.Tickets.First(p => p.Id > 0).Description)</th>
                                                                <th>@Html.DisplayNameFor(model => model.UserInfo.Tickets.First(p => p.Id > 0).Project.Title)</th>
                                                                <th>@Html.DisplayNameFor(model => model.UserInfo.Tickets.First(p => p.Id > 0).AssignedUser)</th>
                                                                <th>@Html.DisplayNameFor(model => model.UserInfo.Tickets.First(p => p.Id > 0).Created)</th>
                                                                <th>@Html.DisplayNameFor(model => model.UserInfo.Tickets.First(p => p.Id > 0).Updated)</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var ticket in Model.UserInfo.Tickets) {
                                                                <tr>
                                                                    <td><a href="/Tickets/Details/@ticket.Id">@Html.DisplayFor(modelItem => ticket.Title)</a></td>
                                                                    <td>@Html.DisplayFor(modelItem => ticket.Description)</td>
                                                                    <td>@Html.DisplayFor(modelItem => ticket.Project.Title)</td>
                                                                    <td>@Html.DisplayFor(modelItem => ticket.AssignedUser.FullName)</td>
                                                                    <td>@Html.DisplayFor(modelItem => ticket.Created)</td>
                                                                    <td>@Html.DisplayFor(modelItem => ticket.Updated)</td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                </div>
                                                <!-- // Assigned Tickets Widget END -->
                                            </div>
                                        </div>
                                    </div>
                                    <!-- OVERVIEW TAB SECTION END -->

                                    <!-- EDIT ACCOUNT TAB SECTION -->
                                    <div class="tab-pane widget-body-regular" id="edit-account">

                                        <div class="widget widget-tabs widget-tabs-vertical row row-merge margin-none">

                                            <!-- Widget heading -->
                                            <div class="widget-head col-md-3">
                                                <ul>
                                                    <li class="active"><a class="glyphicons pencil" href="#account-details" data-toggle="tab"><i></i>Account details</a></li>
                                                    <li><a class="glyphicons settings" href="#account-settings" data-toggle="tab"><i></i>Account settings</a></li>
                                                    <li><a class="glyphicons eye_open" href="#privacy-settings" data-toggle="tab"><i></i>Privacy settings</a></li>
                                                </ul>
                                            </div>
